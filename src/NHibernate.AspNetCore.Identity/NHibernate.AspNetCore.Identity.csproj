<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>NHibernate.AspNetCore.Identity</PackageId>
    <PackageVersion>3.1.6</PackageVersion>
    <Authors>beginor</Authors>
    <Owners>beginor</Owners>
    <PackageProjectUrl>https://github.com/nhibernate/NHibernate.AspNetCore.Identity</PackageProjectUrl>
    <PackageReleaseNotes>Switch back to .NETStandard2.0.</PackageReleaseNotes>
    <Title>NHibernate.AspNetCore.Identity</Title>
    <Description>
      ASPNET Core Identity provider that uses NHibernate for storage. (Build in support for PostgreSQL, SqlServer, MySql, Sqlite)
    </Description>
    <PackageLicenseUrl>https://github.com/nhibernate/NHibernate.AspNetCore.Identity/blob/master/LICENSE.txt</PackageLicenseUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Identity.Core" Version="3.1.3" />
    <PackageReference Include="Microsoft.Extensions.Identity.Stores" Version="3.1.3" />
    <PackageReference Include="NHibernate" Version="5.2.7" />
  </ItemGroup>
</Project>
